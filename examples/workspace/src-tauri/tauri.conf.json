{
  "$schema": "../../../core/tauri-config-schema/schema.json",
  "productName": "workspace",
  "version": "0.1.0",
  "identifier": "com.tauri.workspace",
  "build": {
    "frontendDist": ["../index.html"]
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Workspace",
        "width": 800
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../../.icons/32x32.png",
      "../../.icons/128x128.png",
      "../../.icons/128x128@2x.png",
      "../../.icons/icon.icns",
      "../../.icons/icon.ico"
    ]
  }
}
