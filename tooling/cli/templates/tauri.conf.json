{
  "productName": "{{ app_name }}",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "{{ frontend_dist }}",
    "devUrl": "{{ dev_url }}",
    "beforeDevCommand": "{{ before_dev_command }}",
    "beforeBuildCommand": "{{ before_build_command }}"
  },
  "app": {
    "windows": [
      {
        "title": "{{ window_title }}",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
